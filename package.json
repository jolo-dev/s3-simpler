{
  "name": "s3-simpler",
  "version": "0.0.1",
  "description": "Let's be honest. What do you need when working with S3? Right, simply uploading, downloading, and sharing.",
  "type": "module",
  "scripts": {
    "build": "bun",
    "lint": "eslint --fix tests/**/*.ts",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:unit": "vitest --config ./tests/vitest.config.ts",
    "test:integration": "vitest --config ./tests/vitest.config.integ.ts",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "engines": {
    "node": ">= 18"
  },
  "author": {
    "name": "John Nguyen",
    "email": "johnyscrazy@gmail.com",
    "organization": false
  },
  "devDependencies": {
    "@aws-sdk/types": "^3.433.0",
    "@stylistic/eslint-plugin-ts": "^0.1.1",
    "@tsconfig/bun": "^1.0.1",
    "@types/node": "^20.8.9",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "@vitest/coverage-v8": "^0.34.6",
    "@vitest/ui": "^0.34.6",
    "aws-sdk-client-mock": "^3.0.0",
    "bun": "^1.0.7",
    "standard-version": "^9.5.0",
    "typescript": "^5.2.2",
    "vitest": "^v1.0.0-beta.3"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.437.0",
